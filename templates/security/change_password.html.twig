{% extends 'base.html.twig' %}

{% block title %}Change password{% endblock %}

{% block content %}
    <h1>{{ 'Change password'|trans }}</h1>

    {{ form_start(form) }}
        {{ form_widget(form) }}

        <button type="submit" class="btn btn-primary">
            <i class="fa fa-save" aria-hidden="true"></i> {{ 'Save'|trans }}
        </button>
    {{ form_end(form) }}
{% endblock %}

